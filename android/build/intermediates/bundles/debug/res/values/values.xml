    <color name="black_overlay">#66000000</color>
    <declare-styleable name="ButtonBarContainerTheme"><attr format="reference" name="metaButtonBarStyle"/><attr format="reference" name="metaButtonBarButtonStyle"/></declare-styleable>
    <declare-styleable name="ColorPickerView"><attr format="dimension" name="colorViewWidth"/><attr format="dimension" name="colorViewHeight"/><attr format="dimension" name="colorViewMarginLeft"/><attr format="dimension" name="colorViewMarginRight"/><attr format="enum" name="colorViewCheckedType">
            <enum name="dot" value="0"/>
            <enum name="checkMark" value="1"/>
        </attr><attr format="reference" name="colors"/></declare-styleable>
    <declare-styleable name="are"><attr format="enum" name="expandMode">
            <enum name="FULL" value="0"/>
            <enum name="MIN" value="1"/>
        </attr><attr format="boolean" name="hideToolbar"/><attr format="enum" name="toolbarAlignment">
            <enum name="BOTTOM" value="0"/>
            <enum name="TOP" value="1"/>
        </attr></declare-styleable>
    <dimen name="activity_horizontal_margin">8dp</dimen>
    <dimen name="activity_vertical_margin">8dp</dimen>
    <item name="are_scrollview" type="id"/>
    <item name="are_toolbar" type="id"/>
    <item name="emojiPanelId" type="id"/>
    <string name="action_settings">Settings</string>
    <string name="app_name">ARE</string>
    <string name="are_image_insert_hint">Insert an image</string>
    <string name="are_link_insert_hint">Insert a link</string>
    <string name="are_link_title">Link</string>
    <string name="btn_attach_video">Attach Video</string>
    <string name="cancel">Cancel</string>
    <string name="dummy_content">DUMMY\nCONTENT</string>
    <string name="hello_world">Hello world!</string>
    <string name="ok">OK</string>
    <string name="save_to_local">Save to local</string>
    <string name="title_activity_are__video_player">ARE Video</string>
    <string name="title_activity_default_image_preview">ARE DefaultImagePreviewActivity</string>
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="android:textCursorDrawable">@drawable/cursor</item>
    </style>
    <style name="FullscreenActionBarStyle" parent="Widget.AppCompat.ActionBar">
        <item name="android:background">@color/black_overlay</item>
    </style>
    <style name="FullscreenTheme" parent="AppTheme">
        <item name="android:actionBarStyle">@style/FullscreenActionBarStyle</item>
        <item name="android:windowActionBarOverlay">true</item>
        <item name="android:windowBackground">@null</item>
        <item name="metaButtonBarStyle">?android:attr/buttonBarStyle</item>
        <item name="metaButtonBarButtonStyle">?android:attr/buttonBarButtonStyle</item>
    </style>
</resources>